      "post": {
        "summary": "Upload file to remote file system.",
        "operationId": "FilesService_UploadFile",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1UploadFileResponse"
            }
          },
          "default": {
